{
  "name": "keyalesce",
  "version": "2.2.0",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomer@aberba.ch",
    "url": "https://tomeraberba.ch"
  },
  "description": "Get the same key for the same sequence of values!",
  "keywords": [
    "key",
    "key-value",
    "map",
    "idempotent",
    "multiple",
    "composite"
  ],
  "homepage": "https://github.com/TomerAberbach/keyalesce",
  "repository": "TomerAberbach/keyalesce",
  "bugs": {
    "url": "https://github.com/TomerAberbach/keyalesce/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/TomerAberbach"
  },
  "license": "Apache-2.0 AND MIT",
  "files": [
    "dist",
    "license-apache",
    "license-mit",
    "notice-apache"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ --fix .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "bench": "vitest bench",
    "build": "tsdown",
    "prepublishOnly": "pnpm build"
  },
  "prettier": "@tomer/prettier-config",
  "devDependencies": {
    "@fast-check/vitest": "^0.2.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@tomer/eslint-config": "^4.1.1",
    "@tomer/prettier-config": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "jest-extended": "^6.0.0",
    "lfi": "^4.1.1",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "rollup-plugin-tree-shakeable": "^1.0.3",
    "tsdown": "^0.14.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
